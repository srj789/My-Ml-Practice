    "from functools import lru_cache\n",
    "@lru_cache()\n",
    "def fibonacci(n):\n",
    "    if n<2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibonacci(n-1)+fibonacci(n-2)\n",
    "for i in range(20):\n",
    "    print(fibonacci(i))\n",
    "fibonacci.cache_info()"
   ]
